/* --------------------------------------------------------------------- */
stw_opensyde_gui_elements--C_OgeRabDefault::indicator{
width: 18px;
height: 18px;
border-image:url(:/images/RadioButtonInactive.png) 0 0 0 0 stretch stretch;
}

stw_opensyde_gui_elements--C_OgeRabDefault::indicator:checked{
image:url(://images/RadioButtonActiveAlternative.png);
}

stw_opensyde_gui_elements--C_OgeRabProperties::indicator,
stw_opensyde_gui_elements--C_OgeRabCheckBox::indicator{
border-width: 1px;
border-style: solid;
margin-right: 6px;
}

stw_opensyde_gui_elements--C_OgeRabCheckBox::indicator {
width: 30px;
height: 30px;
border-radius: 15px;
}

stw_opensyde_gui_elements--C_OgeRabProperties::indicator {
width: 16px;
height: 16px;
border-radius: 8px;
}
/*stw_opensyde_gui_elements--C_OgeRabCheckBox::indicator:enabled:!checked, alternative radio button look
{
border-radius: 15px;
}
*/

stw_opensyde_gui_elements--C_OgeRabProperties::indicator:enabled:checked,
stw_opensyde_gui_elements--C_OgeRabCheckBox::indicator:enabled:checked {
image:url(:/images/RadioButtonActive.png);
}
