{
margin-right: 8px;
}

stw_opensyde_gui_elements--C_OgeChxSystemCommisioningEdit::indicator:checked
{
width: 24px;
height: 24px;
border-style: none;
border-width: 0px;
}

stw_opensyde_gui_elements--C_OgeChxSystemCommisioningEdit::indicator:!checked
{
