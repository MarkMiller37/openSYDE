/* --------------------------------------------------------------------- */
stw--opensyde_gui_elements--C_CamOgeWiSpinBoxGroupTable QAbstractSpinBox,
stw--opensyde_gui_elements--C_CamOgeSpxProperties,
stw--opensyde_gui_elements--C_FlaOgeSpxDark
{
border-style:solid;
border-width:1px;
padding-left:6px;
padding-right:6px;
}

/* Button offset */
stw--opensyde_gui_elements--C_CamOgeSpxProperties::down-button,
stw--opensyde_gui_elements--C_FlaOgeSpxDark::down-button
{
border-right-width:0px;
border-top-width:1px;
border-left-width:1px;
border-bottom-width:0px;
border-style:solid;
}

stw--opensyde_gui_elements--C_CamOgeSpxProperties::up-button,
stw--opensyde_gui_elements--C_FlaOgeSpxDark::up-button
{
border-right-width:0px;
border-top-width:0px;
border-left-width:1px;
border-bottom-width:1px;
border-style:solid;
}

/* Default spin box icons */
stw--opensyde_gui_elements--C_CamOgeWiSpinBoxGroupTable QAbstractSpinBox::down-button,
stw--opensyde_gui_elements--C_CamOgeSpxProperties::down-button {
image: url(://images/IconSpinBoxDown.svg) 0 0 0 0 stretch stretch;
}

stw--opensyde_gui_elements--C_CamOgeWiSpinBoxGroupTable QAbstractSpinBox::down-button:!enabled,
stw--opensyde_gui_elements--C_CamOgeSpxProperties::down-button:!enabled,
stw--opensyde_gui_elements--C_FlaOgeSpxDark::down-button:!enabled
{
image: url(://images/IconSpinBoxDownDisabled.svg) 0 0 0 0 stretch stretch;
}

stw--opensyde_gui_elements--C_CamOgeWiSpinBoxGroupTable QAbstractSpinBox::up-button,
stw--opensyde_gui_elements--C_CamOgeSpxProperties::up-button {
image: url(://images/IconSpinBoxUp.svg) 0 0 0 0 stretch stretch;
}

stw--opensyde_gui_elements--C_CamOgeWiSpinBoxGroupTable QAbstractSpinBox::up-button:!enabled,
stw--opensyde_gui_elements--C_CamOgeSpxProperties::up-button:!enabled,
stw--opensyde_gui_elements--C_FlaOgeSpxDark::up-button:!enabled
{
image: url(://images/IconSpinBoxUpDisabled.svg) 0 0 0 0 stretch stretch;
}

/* Dark spin box icons */
stw--opensyde_gui_elements--C_FlaOgeSpxDark::down-button {
image: url(://images/IconSpinBoxDownDark.svg) 0 0 0 0 stretch stretch;
}

stw--opensyde_gui_elements--C_FlaOgeSpxDark::up-button {
image: url(://images/IconSpinBoxUpDark.svg) 0 0 0 0 stretch stretch;
}

